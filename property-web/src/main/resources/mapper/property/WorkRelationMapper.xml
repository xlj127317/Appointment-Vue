<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.property.mapper.WorkRelationMapper">
    <resultMap id="BaseResultMap" type="com.ruoyi.property.domain.WorkRelation">
        <!--@mbg.generated-->
        <!--@Table work_relation-->
        <id column="work_id" jdbcType="VARCHAR" property="workId"/>
        <result column="task_id" jdbcType="VARCHAR" property="taskId"/>
    </resultMap>
    <sql id="Base_Column_List">
        <!--@mbg.generated-->
        work_id,
        task_id
    </sql>

    <insert id="insert">
        INSERT INTO work_relation(work_id, task_id)
        VALUES (#{workId}, #{taskId})
    </insert>

    <select id="selectByTaskId" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from work_relation
        where task_id = #{id}
    </select>
</mapper>